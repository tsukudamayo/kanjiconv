        "instructionServings": [
            {
                "unit": "2人",
                "instructions": [
                    {
                        "step": 1,
                        "description": "〈くるみディップ〉"
                    },
                    {
                        "step": 2,
                        "description": "くるみは１５０度のオーブンで約７分焼くか、弱火にかけたフライパンでから焼きし、あらみじんに切ります。"
                    },
                    {
                        "step": 3,
                        "description": "レーズンはぬるま湯で洗い、あらみじんに切ります。"
                    },
                    {
                        "step": 4,
                        "description": "チーズ１／３箱（約２６．７ｇ）と材料全部を混ぜ合わせます。"
                    },
                    {
                        "step": 5,
                        "description": "〈めんたいディップ〉"
                    },
                    {
                        "step": 6,
                        "description": "めんたいこは薄皮を除き、レモン汁を混ぜます。たまねぎはみじん切りにして水にさらし、水気をしぼります。"
                    },
                    {
                        "step": 7,
                        "description": "チーズ１／３箱（約２６．７ｇ）と材料全部を混ぜ合わせます。"
                    },
                    {
                        "step": 8,
                        "description": "〈しらすディップ〉"
                    },
                    {
                        "step": 9,
                        "description": "しらすは熱湯をかけます。ごまはあらく切ります。しそは細切りにして水にさらし、水気をしぼります。"
                    },
                    {
                        "step": 10,
                        "description": "チーズ１／３箱（約２６．７ｇ）と材料全部を混ぜ合わせます。牛乳の量は、やわらかさによって加減します。"
                    }
                ]
            },
